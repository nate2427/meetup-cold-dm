{
  "manifest_version": 3,
  "name": "Meetup Mass Messenger",
  "description": "Messages Meetup Group Members",
  "version": "1.0",
  "action": {
    "default_popup": "send_message.html",
    "default_icon": "streetcoderslogo.PNG"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://meetup.com/*", "https://secure.meetup.com/*"]
}
